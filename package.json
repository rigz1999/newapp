{
  "name": "finixar",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit -p tsconfig.app.json",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "generate:template": "node scripts/generate-excel-template.js",
    "prepare": "husky",
    "loadtest:smoke": "k6 run load-tests/tests/smoke-test.js",
    "loadtest:api": "k6 run load-tests/tests/api-endpoints.js",
    "loadtest:flows": "k6 run load-tests/tests/user-flows.js",
    "loadtest:stress": "k6 run load-tests/tests/stress-test.js",
    "loadtest:spike": "k6 run load-tests/tests/spike-test.js",
    "loadtest:soak": "k6 run load-tests/tests/soak-test.js",
    "loadtest:all": "npm run loadtest:smoke && npm run loadtest:api && npm run loadtest:flows"
  },
  "dependencies": {
    "@sentry/react": "^10.23.0",
    "@sentry/vite-plugin": "^4.6.0",
    "@supabase/supabase-js": "^2.57.4",
    "date-fns": "^4.1.0",
    "decimal.js": "^10.6.0",
    "dompurify": "^3.3.0",
    "exceljs": "^4.4.0",
    "jspdf": "^4.0.0",
    "jspdf-autotable": "^5.0.2",
    "lucide-react": "^0.344.0",
    "pdfjs-dist": "^5.4.296",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.9.4",
    "sonner": "^2.0.7",
    "web-vitals": "^5.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/dompurify": "^3.0.5",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/ui": "^4.0.7",
    "autoprefixer": "^10.4.18",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "happy-dom": "^20.0.10",
    "husky": "^9.1.7",
    "jsdom": "^27.1.0",
    "lint-staged": "^16.2.6",
    "msw": "^2.12.0",
    "postcss": "^8.4.35",
    "prettier": "^3.6.2",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^7.2.1",
    "vitest": "^4.0.7"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{js,jsx,json,css,md}": [
      "prettier --write"
    ]
  }
}
